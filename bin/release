#!/usr/bin/env bash
# bin/release <build-dir>

BUILD_DIR=$1

cat <<EOF
---
config_vars:
  PATH: .maven/bin:/usr/local/bin:/usr/bin:/bin
  REPO: /app/.m2/repository
  JAVA_OPTS: -Xmx384m -Xss256k -XX:+UseCompressedOops
  MAVEN_OPTS: -Xmx384m -Xss256k -XX:+UseCompressedOops 
addons:
  shared-database:5mb


EOF
